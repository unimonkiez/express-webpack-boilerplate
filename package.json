{
  "name": "express-webpack-boilerplate",
  "version": "0.0.1",
  "description": "express-webpack-boilerplate",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build:prod",
    "lint": "eslint --ext .js,.jsx .",
    "test": "npm run lint",
    "build": "node ./bin/build.js",
