    "json-loader": "^0.5.4",
    "material-ui": "^0.17.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-tap-event-plugin": "^2.0.1",
    "replace-loader": "^0.2.0",
