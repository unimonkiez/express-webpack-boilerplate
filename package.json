    "build:watch:prod": "npm run build:prod:watch",
    "start": "npm run start:prod -- -w",
    "start:prod": "node ./index.js"
  },
  "author": "unimonkiez",
  "repository": {
    "type": "git",
    "url": "git@github.com:unimonkiez/express-webpack-boilerplate.git"
  },
